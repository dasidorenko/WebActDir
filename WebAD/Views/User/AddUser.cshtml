
@{
    ViewBag.Title = "AddUser";
}

<h2>AddUser</h2>

<form method="post" action="AddUser">
    <p>Введите имя </p>
    <input type="text" name="name" />
    <p>Введите фамилию :</p>
    <input type="text" name="surname" />
    <p>Введите логин :</p>
    <input type="text" name="login" />
    <p>Введите пароль :</p>
    <input type="text" name="password" />
    <input type="submit" value="Отправить" />
</form>